<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_dialog"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="120dp"
    android:paddingTop="100dp"
    tools:context="com.hp.technicalfest.DialogActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <TextView
            android:text="Wid :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/extviewwid"
            android:textSize="20sp"
            />

        <TextView
            android:text=" TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:id="@+id/dwid"
             />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <TextView
            android:text="Name :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/extviewwname"
            android:textSize="20sp"
            />

        <TextView
            android:text=" TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:id="@+id/dwname"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <TextView
            android:text="Type :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/extviewwype"
            android:textSize="20sp"
            />

        <TextView
            android:text=" TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:id="@+id/dwtype"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <TextView
            android:text="Venue :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/extviewwveue"
            android:textSize="20sp"
            />

        <TextView
            android:text=" TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:id="@+id/dwvenue"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <TextView
            android:text="Time :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/extviewwtime"
            android:textSize="20sp"
            />

        <TextView
            android:text=" TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:id="@+id/dwtime"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <TextView
            android:text="Price :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/extviewwprice"
            android:textSize="20sp"
            />

        <TextView
            android:text=" TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:id="@+id/dwprice"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <TextView
            android:text="Price :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/extviewwstatus"
            android:textSize="20sp"
            />

        <TextView
            android:text=" TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:id="@+id/dwstatus"
            />
    </LinearLayout>
</LinearLayout>
